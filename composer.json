{
    "name": "davidjr82/php-pdf-generator",
    "description": "Yet another php pdf generator package",
    "keywords": [
        "test",
        "package"
    ],
    "homepage": "https://github.com/test/package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Jim√©nez",
            "email": "davidjr82@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/process": "^5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.18",
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "Davidjr82\\PhpPDFGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Davidjr82\\PhpPDFGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --colors=always --coverage-html .code_coverage",
        "cs": "./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php -v --using-cache=no"
    },
    "config": {
        "sort-packages": true
    }
}
